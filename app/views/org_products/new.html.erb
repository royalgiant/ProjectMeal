<% content_for(:sidebar) do %>
    <%= render :partial => "layouts/sidebar" %>
<% end %> 
<div class="col-md-7 col-md-offset-3 main">
<%= provide(:title, "Post Food Item") %>
<center><h1>Post Food for Sale</h1></center>
	
		<%= form_for(@product) do |f| %>
			<%= render 'shared/error_messages' %>
   			<div class="col-md-12 ">
   				<%= render 'layouts/productfields', f: f %>
     	 		<%= f.submit "Post Product", class: "btn btn-large btn-primary" %>
   			</div>
		<% end %>
</div>